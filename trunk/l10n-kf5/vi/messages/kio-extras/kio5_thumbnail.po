#: jpegcreator.cpp:48
#, kde-format
msgctxt "@option:check"
msgid "Rotate the image automatically"
msgstr "Xoay ảnh tự động"

#: thumbnail.cpp:173
#, kde-format
msgid "No MIME Type specified."
msgstr "Chưa chỉ ra kiểu MIME."

#: thumbnail.cpp:181
#, kde-format
msgid "No or invalid size specified."
msgstr "Chưa chỉ ra kích thước, hoặc chỉ ra kích thước sai."

#: thumbnail.cpp:202
#, kde-format
msgid "Cannot create thumbnail for directory"
msgstr "Không tạo được ảnh xem trước cho thư mục"

#: thumbnail.cpp:214
#, kde-format
msgid "No plugin specified."
msgstr "Chưa chỉ định phần bổ sung."

#: thumbnail.cpp:220
#, kde-format
msgid "Cannot load ThumbCreator %1"
msgstr "Không nạp ThumbCreator %1"

#: thumbnail.cpp:232
#, kde-format
msgid "Cannot create thumbnail for %1"
msgstr "Không tạo được ảnh xem trước cho %1"

#: thumbnail.cpp:247
#, kde-format
msgid "Failed to create a thumbnail."
msgstr "Thất bại khi tạo một ảnh xem trước."

#: thumbnail.cpp:259
#, kde-format
msgid "Could not write image."
msgstr "Không ghi được ảnh."

#: thumbnail.cpp:284
#, kde-format
msgid "Failed to attach to shared memory segment %1"
msgstr "Lỗi gắn tới đoạn bộ nhớ chia sẻ %1"

#: thumbnail.cpp:288
#, kde-format
msgid "Image is too big for the shared memory segment"
msgstr "Ảnh quá lớn đối với đoạn bộ nhớ chia sẻ"
